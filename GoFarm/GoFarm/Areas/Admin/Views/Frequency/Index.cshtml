
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br />

<div class="border backgroundWhite">
    <div class="row">
        <div class="col-6 pl-5 p-4">
            <h2 class="text-primary"> Frequency List</h2>
        </div>
        <div class="col-6 text-right pr-5 p-4">
            <a asp-action="Upsert" class="btn btn-primary">
                <i class="fa fa-plus"></i>&nbsp; Create New Frequency 
            </a>
        </div>
    </div>

    <br />
    <br />

    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Description
                </th>
                <th></th>
            </tr>
        </thead>
    </table>

</div>
@section Scripts{

    <script src="~/js/frequency.js"></script>

}

